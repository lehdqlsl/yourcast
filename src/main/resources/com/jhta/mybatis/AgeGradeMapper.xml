<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jhta.mybatis.AgeGradeMapper">
	<select id="getInfo" parameterType="int" resultType="agegrade">
		select * from age_grade where age_grade_num=#{age_grade_num}
	</select>
	<select id="getList" resultType="agegrade">
		select * from age_grade
	</select>
</mapper>